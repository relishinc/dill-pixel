{
  "name": "dill-pixel-examples-v8",
  "version": "0.0.1",
  "description": "Examples of Dill Pixel Features (V8)",
  "main": "index.html",
  "scripts": {
    "clean": "rimraf dist .assetpack .cache",
    "assets": "assetpack -c .assetpack.mjs",
    "cache": "rimraf .cache",
    "start": "run-s cache && vite",
    "dev": "run-s cache assets && vite",
    "prebuild": "run-s clean format lint assets",
    "build": "vite build",
    "types": "tsc",
    "preview": "vite preview --open",
    "lint": "eslint src --ext .ts",
    "lint-fix": "eslint src --ext .ts --fix",
    "visualize": "npx vite-bundle-visualizer -c vite.config.mjs",
    "audio:vo:en": "texttospeech assets/audio{m}/vo/en --sheet-name=\"Intro EN\"",
    "audio:vo:fr": "texttospeech assets/audio{m}/vo/fr --sheet-name=\"Intro FR\"",
    "audio:captions:en": "node ../cli.mjs audio captions src/assets/audio/en assets/audio{m}/vo/en",
    "audio:captions:fr": "node ../cli.mjs audio captions src/assets/audio/fr assets/audio{m}/vo/fr",
    "vo:generate": "node ../cli.mjs vo generate",
    "spritesheets": "node assets/images/export_spritesheets",
    "format": "prettier --write src",
    "format:check": "prettier --check src"
  },
  "author": "Relish Digital<dev@reli.sh>",
  "license": "MIT",
  "devDependencies": {
    "@relishinc/text-to-speech": "^1.4.0",
    "@types/dat.gui": "^0.7.13"
  },
  "dependencies": {
    "@dill-pixel/plugin-arcade-physics": "file:../packages/plugins/physics/arcade",
    "@dill-pixel/plugin-snap-physics": "file:../packages/plugins/physics/snap",
    "@dill-pixel/plugin-matter-physics": "file:../packages/plugins/physics/matter",
    "@dill-pixel/plugin-rive": "file:../packages/plugins/rive",
    "@dill-pixel/plugin-rollbar": "file:../packages/plugins/rollbar",
    "@dill-pixel/storage-adapter-firebase": "file:../packages/storage-adapters/firebase",
    "dat.gui": "^0.7.9",
    "dill-pixel": "file:..",
    "firebase": "file:../packages/storage-adapters/firebase/node_modules/firebase"
  }
}
