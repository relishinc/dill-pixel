{
  "name": "dill-pixel-examples-v8",
  "version": "0.0.0",
  "description": "Examples of Dill Pixel Features (V8)",
  "main": "index.html",
  "scripts": {
    "clean": "rimraf dist/* public/* .assetpack/*",
    "prestart": "run-s assets",
    "predev": "run-s assets",
    "assets": "assetpack -c .assetpack.mjs",
    "start": "vite",
    "dev": "vite",
    "prebuild": "run-s clean format:check lint assets types",
    "build": "vite build",
    "prepreview": "run-s build",
    "preview": "vite preview --open",
    "lint": "eslint src --ext .ts",
    "lint-fix": "eslint src --ext .ts --fix",
    "audio:vo": "texttospeech",
    "audio:captions": "node ../cli.mjs audio captions assets/audio",
    "audio:compress": "node ../cli.mjs audio compress assets/audio",
    "spritesheets": "node assets/images/export_spritesheets"
  },
  "author": "Relish Digital<dev@reli.sh>",
  "license": "MIT",
  "devDependencies": {
    "@types/dat.gui": "^0.7.11",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "prettier": "^3.0.3",
    "relishinc-prettier": "github:relishinc/prettier",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@pixi/ui": "^0.9.0",
    "dat.gui": "^0.7.9",
    "dill-pixel": "file:..",
    "gsap": "^3.11.5"
  }
}
