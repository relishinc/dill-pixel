{
  "name": "dill-pixel",
  "version": "2.27.6",
  "title": "Dill Pixel",
  "description": "Relish framework for HTML games",
  "main": "src/index.ts",
  "bin": {
    "dill-pixel": "./cli.mjs",
    "create-dill-pixel": "./cli/create-dill-pixel.mjs"
  },
  "types": "./lib/index.d.ts",
  "exports": {
    ".": "./src/index.ts",
    "./react": "./src/react/index.ts"
  },
  "sideEffects": false,
  "scripts": {
    "dev": "cd ./examples && npm run dev",
    "dev:react": "cd ./examples-react && npm run dev",
    "build:react": "cd ./examples-react && npm run build",
    "build": "npm run version && npm run compile",
    "compile": "tsc",
    "build:examples": "cd ./examples && npm run build",
    "visualize:examples": "cd ./examples && npx vite-bundle-visualizer",
    "build:watch": "tsc --watch",
    "lint": "eslint --ext .ts ./src/**/*",
    "docs": "npm run build:docs",
    "build:docs": "cd ./docs && npm run build",
    "dev:docs": "cd ./docs && npm run dev",
    "typedoc": "npm run version && typedoc --out typedoc src",
    "version": "node ./scripts/generate-version.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/relishinc/hlf.git"
  },
  "keywords": [
    "game",
    "framework",
    "html"
  ],
  "author": "Relish Interactive",
  "license": "MIT",
  "homepage": "https://github.com/relishinc/hlf#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "relishinc-prettier": "github:relishinc/prettier",
    "standard-version": "^9.5.0",
    "tslint": "^6.1.3",
    "typedoc": "^0.25.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "@dimforge/rapier2d": "^0.11.2",
    "@pixi/filter-outline": "^5.2.0",
    "@pixi/react": "latest",
    "@pixi/react-animated": "latest",
    "@types/howler": "^2.2.9",
    "@types/matter-js": "^0.19.1",
    "@types/stats.js": "^0.17.1",
    "csv-parse": "^5.5.3",
    "fontfaceobserver": "^2.3.0",
    "gsap": "^3.12.1",
    "howler": "^2.2.3",
    "keycode": "^2.2.1",
    "kleur": "^4.1.5",
    "matter-js": "^0.19.0",
    "pixi-spine": "^4.0.4",
    "pixi.js": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-fps-stats": "^0.3.1",
    "react-spring": "latest",
    "stats.js": "^0.17.0",
    "typed-signals": "^2.5.0",
    "typescript-collections": "^1.3.3",
    "zustand": "^4.4.1"
  }
}
