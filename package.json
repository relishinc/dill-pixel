{
  "name": "dill-pixel",
  "version": "2.5.0",
  "description": "Relish framework for HTML games",
  "main": "./src/index.ts",
  "sideEffects": false,
  "scripts": {
    "dev": "cd ./examples && npm run dev",
    "dev:react": "cd ./examples-react && npm run dev",
    "build": "tsc",
    "build:examples": "cd ./examples && npm run build",
    "visualize:examples": "cd ./examples && npx vite-bundle-visualizer",
    "build:watch": "tsc --watch",
    "lint": "eslint --ext .ts ./src/**/*",
    "docs": "npm run version && typedoc --out docs src",
    "copy": "copyfiles ./src/**/* ../hlf-example-template/relish-hlf",
    "version": "node build_scripts/version.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/relishinc/hlf.git"
  },
  "keywords": [
    "game",
    "dev",
    "html"
  ],
  "author": "Relish Interactive",
  "license": "MIT",
  "homepage": "https://github.com/relishinc/hlf#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@types/howler": "^2.2.7",
    "@types/matter-js": "^0.18.5",
    "@types/stats.js": "^0.17.1",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "react-fps-stats": "^0.3.1",
    "relishinc-prettier": "github:relishinc/prettier",
    "standard-version": "^9.5.0",
    "stats.js": "^0.17.0",
    "tslint": "^6.1.3",
    "typedoc": "^0.25.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@dimforge/rapier2d": "^0.11.2",
    "@pixi/filter-outline": "^5.2.0",
    "@pixi/react": "^7.1.1",
    "@pixi/react-animated": "^7.1.1",
    "fontfaceobserver": "^2.3.0",
    "gsap": "^3.12.1",
    "howler": "^2.2.3",
    "keycode": "^2.2.1",
    "matter-js": "^0.19.0",
    "pixi-spine": "^4.0.4",
    "pixi.js": "^7.x",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-spring": "^9.7.3",
    "typed-signals": "^2.5.0",
    "typescript-collections": "^1.3.3",
    "zustand": "^4.4.1"
  }
}
