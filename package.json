{
  "name": "@relish-studios/dill-pixel",
  "version": "8.0.0",
  "title": "Dill Pixel",
  "description": "Relish framework for HTML games",
  "main": "lib/index.js",
  "bin": {
    "dill-pixel": "./cli.mjs",
    "create-dill-pixel": "./cli/create-dill-pixel.mjs"
  },
  "types": "./lib/index.d.ts",
  "exports": {
    ".": "./src/index.ts",
    "./html-loader": "./src/css/html-loader.css",
    "./vite": "./config/vite.mjs",
    "./tsconfig": "./config/tsconfig.base.json",
    "./assetpack": "./config/assetpack.mjs"
  },
  "sideEffects": false,
  "scripts": {
    "dev": "cd ./examples && npm run dev",
    "start": "cd ./examples && npm run start",
    "build": "npm run version && npm run pixi-exports && npm run generate && npm run compile",
    "build:quick": "npm run compile",
    "pixi-exports": "node ./scripts/pixi-exports.js",
    "compile": "tsc",
    "build:examples": "cd ./examples && npm run build",
    "visualize:examples": "cd ./examples && npx vite-bundle-visualizer",
    "build:watch": "tsc --watch",
    "lint": "eslint --ext .ts ./src/**/*",
    "lint:fix": "eslint --ext .ts ./src/**/* --fix",
    "docs": "npm run build:docs",
    "build:docs": "cd ./docs && npm run build",
    "dev:docs": "cd ./docs && npm run dev",
    "typedoc": "npm run version && typedoc --out typedoc src",
    "version": "node ./scripts/generate-version.js",
    "generate": "npm run generate:core-functions && npm run generate:core-signals",
    "generate:core-functions": "node scripts/generate-core-functions.mjs",
    "generate:core-signals": "node scripts/generate-core-signals.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/relishinc/hlf.git"
  },
  "keywords": [
    "game",
    "framework",
    "html"
  ],
  "author": "Relish Interactive",
  "license": "MIT",
  "homepage": "https://github.com/relishinc/dill-pixel#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@types/matter-js": "^0.19.6",
    "@types/stats.js": "^0.17.3",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^8.0.3",
    "prettier": "^3.2.5",
    "relishinc-prettier": "github:relishinc/prettier",
    "standard-version": "^9.5.0",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@assetpack/cli": "^0.8.0",
    "@assetpack/core": "^0.8.0",
    "@assetpack/plugin-compress": "^0.8.0",
    "@assetpack/plugin-ffmpeg": "^0.8.0",
    "@assetpack/plugin-json": "^0.8.0",
    "@assetpack/plugin-manifest": "^0.8.0",
    "@assetpack/plugin-mipmap": "^0.8.0",
    "@assetpack/plugin-texture-packer": "^0.8.0",
    "@assetpack/plugin-webfont": "^0.8.0",
    "@clack/prompts": "^0.7.0",
    "@dimforge/rapier2d": "^0.12.0",
    "@esotericsoftware/spine-core": "^4.2.39",
    "@pixi/sound": "^6.0.0",
    "@pixi/spine-pixi": "^1.0.8",
    "@vitejs/plugin-legacy": "^5.3.2",
    "csv-parse": "^5.5.5",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "gsap": "^3.12.5",
    "kleur": "^4.1.5",
    "matter-js": "^0.19.0",
    "npm-run-all": "^4.1.5",
    "pixi-stats": "^1.3.1",
    "pixi.js": "^8.1.1",
    "rimraf": "^5.0.5",
    "stats.js": "^0.17.0",
    "typed-signals": "^2.5.0",
    "vite": "^5.2.10",
    "vite-bundle-visualizer": "^1.1.0",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-singlefile": "^0.13.5",
    "vite-plugin-static-copy": "^1.0.3",
    "vite-plugin-top-level-await": "^1.4.1",
    "vite-plugin-wasm": "^3.3.0"
  }
}
