{
  "name": "html-living-framework",
  "version": "2.3.0",
  "description": "Relish framework for HTML games",
  "main": "./src/index.ts",
  "sideEffects": false,
  "scripts": {
    "dev": "cd ./examples && npm run dev",
    "build": "tsc",
    "build:examples": "cd ./examples && npm run build",
    "visualize:examples": "cd ./examples && npx vite-bundle-visualizer",
    "build:watch": "tsc --watch",
    "lint": "tslint -c tslint.json \"src/**/*.ts\"",
    "docs": "npm run version && typedoc --out docs src",
    "copy": "copyfiles ./src/**/* ../hlf-example-template/relish-hlf",
    "version": "node build_scripts/version.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/relishinc/hlf.git"
  },
  "keywords": [
    "game",
    "dev",
    "html"
  ],
  "author": "Relish Interactive",
  "license": "UNLICENSED",
  "homepage": "https://github.com/relishinc/hlf#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@types/howler": "^2.2.7",
    "@types/matter-js": "^0.18.5",
    "husky": "^8.0.3",
    "standard-version": "^9.5.0",
    "tslint": "^6.1.3",
    "typedoc": "^0.24.8",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@dimforge/rapier2d": "^0.11.2",
    "@pixi/filter-outline": "^5.2.0",
    "fontfaceobserver": "^2.3.0",
    "gsap": "^3.12.1",
    "howler": "^2.2.3",
    "keycode": "^2.2.1",
    "matter-js": "^0.19.0",
    "pixi-spine": "^4.0.4",
    "pixi.js": "^7.2.4",
    "typed-signals": "^2.5.0",
    "typescript-collections": "^1.3.3"
  }
}
