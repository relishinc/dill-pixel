{
  "name": "dill-pixel-examples-v8",
  "version": "8.12.0",
  "description": "Examples of Dill Pixel Features (V8)",
  "main": "index.html",
  "scripts": {
    "clean": "rimraf dist .assetpack .cache",
    "assets": "assetpack -c .assetpack.mjs",
    "cache": "rimraf .cache",
    "start": "run-s cache && vite",
    "dev": "run-s cache assets && vite",
    "prebuild": "run-s clean format lint",
    "build": "vite build",
    "types": "tsc",
    "preview": "vite preview --open",
    "lint": "eslint src --ext .ts",
    "lint-fix": "eslint src --ext .ts --fix",
    "visualize": "npx vite-bundle-visualizer -c vite.config.mjs",
    "audio:vo:en": "texttospeech assets/audio{m}/vo/en --sheet-name=\"Intro EN\"",
    "audio:vo:fr": "texttospeech assets/audio{m}/vo/fr --sheet-name=\"Intro FR\"",
    "audio:captions:en": "node ../cli.mjs audio captions src/assets/audio/en assets/audio{m}/vo/en",
    "audio:captions:fr": "node ../cli.mjs audio captions src/assets/audio/fr assets/audio{m}/vo/fr",
    "vo:generate": "node ../cli.mjs vo generate",
    "spritesheets": "node assets/images/export_spritesheets",
    "format": "prettier --write src",
    "format:check": "prettier --check src"
  },
  "author": "Relish Digital<dev@reli.sh>",
  "license": "MIT",
  "devDependencies": {
    "@relishinc/text-to-speech": "^1.4.0",
    "@types/dat.gui": "^0.7.13"
  },
  "dependencies": {
    "@assetpack/cli": "0.8.0",
    "@assetpack/core": "0.8.0",
    "@assetpack/plugin-compress": "0.8.0",
    "@assetpack/plugin-ffmpeg": "0.8.0",
    "@assetpack/plugin-json": "0.8.0",
    "@assetpack/plugin-manifest": "0.8.0",
    "@assetpack/plugin-mipmap": "0.8.0",
    "@assetpack/plugin-texture-packer": "0.8.0",
    "@assetpack/plugin-webfont": "0.8.0",
    "@dill-pixel/plugin-arcade-physics": "workspace:*",
    "@dill-pixel/plugin-matter-physics": "workspace:*",
    "@dill-pixel/plugin-rive": "workspace:*",
    "@dill-pixel/plugin-rollbar": "workspace:*",
    "@dill-pixel/plugin-snap-physics": "workspace:*",
    "@dill-pixel/storage-adapter-firebase": "workspace:*",
    "@pixi/sound": "6.0.0",
    "dat.gui": "^0.7.9",
    "dill-pixel": "workspace:*",
    "firebase": "^10.12.2",
    "gsap": "3.12.5",
    "npm-run-all": "4.1.5",
    "pixi.js": "8.2.1",
    "rimraf": "5.0.5",
    "stats.js": "0.17.0",
    "vite": "5.2.13",
    "vite-bundle-visualizer": "1.1.0",
    "vite-plugin-html": "3.2.2",
    "vite-plugin-singlefile": "0.13.5",
    "vite-plugin-static-copy": "1.0.3",
    "vite-plugin-top-level-await": "1.4.1",
    "vite-plugin-wasm": "3.3.0"
  },
  "packageManager": "pnpm@9.3.0"
}
