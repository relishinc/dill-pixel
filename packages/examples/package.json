{
  "name": "dill-pixel-examples-v8",
  "version": "4.15.6",
  "description": "Examples of Dill Pixel Features (V8)",
  "main": "index.html",
  "scripts": {
    "clean": "rimraf dist .assetpack .cache",
    "assets": "assetpack -c .assetpack.mjs",
    "cache": "rimraf .cache",
    "start": "run-s cache && vite --open",
    "dev": "run-s cache && vite --open",
    "build": "vite build",
    "types": "tsc",
    "preview": "pnpm run clean && pnpm run build && vite preview --open",
    "lint": "eslint src --ext .ts",
    "lint-fix": "eslint src --ext .ts --fix",
    "visualize": "npx vite-bundle-visualizer -c vite.config.mjs",
    "audio:vo:en": "texttospeech assets/audio{m}/vo/en --sheet-name=\"Intro EN\"",
    "audio:vo:fr": "texttospeech assets/audio{m}/vo/fr --sheet-name=\"Intro FR\"",
    "audio:captions:en": "node ../framework/cli.mjs audio captions src/assets/audio/en assets/audio{m}/vo/en",
    "audio:captions:fr": "node ../framework/cli.mjs audio captions src/assets/audio/fr assets/audio{m}/vo/fr",
    "vo:generate": "node ../framework/cli.mjs vo generate \"./src/locales, ./assets/locales\"",
    "spritesheets": "node assets/images/export_spritesheets",
    "format": "prettier --write src",
    "format:check": "prettier --check src"
  },
  "author": "Relish Digital<dev@reli.sh>",
  "license": "MIT",
  "devDependencies": {
    "@types/dat.gui": "^0.7.13"
  },
  "dependencies": {
    "@assetpack/core": "^1.3.0",
    "@dill-pixel/plugin-crunch-physics": "workspace:*",
    "@dill-pixel/plugin-google-analytics": "workspace:*",
    "@dill-pixel/plugin-matter-physics": "workspace:*",
    "@dill-pixel/plugin-rive": "workspace:*",
    "@dill-pixel/plugin-rollbar": "workspace:*",
    "@dill-pixel/plugin-snap-physics": "workspace:*",
    "@dill-pixel/plugin-springroll": "workspace:*",
    "@dill-pixel/storage-adapter-firebase": "workspace:*",
    "@dill-pixel/storage-adapter-supabase": "workspace:*",
    "@pixi/sound": "^6.0.1",
    "dat.gui": "^0.7.9",
    "dill-pixel": "workspace:*",
    "firebase": "^10.12.2",
    "gsap": "3.12.5",
    "npm-run-all": "4.1.5",
    "pixi.js": "8.9.1",
    "rimraf": "5.0.5",
    "stats.js": "0.17.0",
    "vite": "6.1.2",
    "vite-bundle-visualizer": "1.2.1",
    "vite-plugin-html": "3.2.2",
    "vite-plugin-singlefile": "2.0.3",
    "vite-plugin-static-copy": "2.2.0",
    "vite-plugin-top-level-await": "1.4.4",
    "vite-plugin-wasm": "3.3.0"
  },
  "packageManager": "pnpm@9.3.0"
}
